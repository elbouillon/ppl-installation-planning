jQuery ($) ->
  date = new Date()
  d = date.getDate()
  m = date.getMonth()
  y = date.getFullYear()

  $('#calendar').fullCalendar
    header:
      left: 'prev,next today'
      center: 'title'
      right: 'month,basicWeek'

    firstDay: 1
    weekends: false
    editable: true
    events: '/installations.json'
